{
  "name": "gx-geminis-ds",
  "version": "0.1.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/gx-geminis-ds/gx-geminis-ds.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve --docs",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 9001 -c .storybook -s www",
    "story": "npm-run-all --parallel start storybook",
    "validate": "npm run test && npm run build",
    "validate.ci": "npm run test && npm run build --max-workers 1 --debug",
    "build-storybook": "build-storybook -c .storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@stencil/core": "^1.7.4",
    "@stencil/sass": "^1.1.0",
    "@storybook/addon-notes": "^5.2.5",
    "@storybook/html": "^5.2.5",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "npm-run-all": "^4.1.5",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "license": "MIT",
  "dependencies": {
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-storysource": "^5.2.1"
  }
}
